<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
   <!--解决乱码    -->
	    <constant name="struts.i18n.encoding" value="UTF-8"></constant>

	<package name="user" namespace="/" extends="struts-default,json-default">
	   <action name="user_*" method="{1}" class="userAction">
	      <result name="dologin">/WEB-INF/login/login.jsp</result>
	      <result name="list" >/WEB-INF/user/list.jsp</result>
	      <result name="registered">/WEB-INF/user/User_Registered.jsp</result>
	      <result name="updateEquipment">/WEB-INF/Equipment/updateEquipment.jsp</result>
	      <result name="equipmentList">/WEB-INF/Equipment/equipmentList.jsp</result>  
	      <result name="load">/WEB-INF/Equipment/load.jsp</result>  
	      <result name="equipmentInfo">/WEB-INF/Equipment/equipmentInfo.jsp</result>
	      <result name="equipment">/WEB-INF/Equipment/equipmentList.jsp</result>  
	      <result name="User_Management">/WEB-INF/user/User_Management.jsp</result>
	       <result name="User_Permissions">/WEB-INF/user/User_Permissions.jsp</result>
	        <result name="User_PersonalData">/WEB-INF/user/User_PersonalData.jsp</result>
	        <result name="PersonalInf">/WEB-INF/user/User_PersonalData_PersonalInf.jsp</result>
	        <result name="Password">/WEB-INF/user/User_PersonalData_Password.jsp</result>
	        <result name="updateUser">/WEB-INF/user/User_UpdateUser.jsp</result>
	        <result name="updatePermissions">/WEB-INF/user/User_UpdatePermissions.jsp</result>  
	        <result name="accredit">/WEB-INF/user/User_Accredit.jsp</result>  
	        <result name="doquit">/WEB-INF/login/login.jsp</result>
	   </action>
	</package>
</struts>
