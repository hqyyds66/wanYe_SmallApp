package com.mindor.entity;

import java.io.Serializable;
import java.util.HashSet;
import java.util.Set;

@SuppressWarnings("serial")
public class Product implements Serializable {
	private String productId;// 产品ID
	private String productName;// 产品名称
	private String industry;// 产品行业
	private String category;// 产品类别
	private String introduce;// 产品简介
	private String category02;// 产品类别2
	private String category03;// 产品类别3
	private String agreement;// 设备接入协议
	private String date;// 创建时间
	private String productImage;// 产品图片
	private String productIcon;// 产品图标
	private OpenUser openUser;// 绑定用户信息
	private String softVersion;// 软件版本
	private String version_updateContent;// 更新内容

	public String getVersion_updateContent() {
		return version_updateContent;
	}

	public void setVersion_updateContent(String version_updateContent) {
		this.version_updateContent = version_updateContent;
	}

	public String getVersion_updateTime() {
		return version_updateTime;
	}

	public void setVersion_updateTime(String version_updateTime) {
		this.version_updateTime = version_updateTime;
	}

	private String version_updateTime;// 版本更新时间

	public String getSoftVersion() {
		return softVersion;
	}

	public void setSoftVersion(String softVersion) {
		this.softVersion = softVersion;
	}

	private Set<Equipment> equipment = new HashSet<Equipment>();

	public Set<Equipment> getEquipment() {
		return equipment;
	}

	public void setEquipment(Set<Equipment> equipment) {
		this.equipment = equipment;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public String getIndustry() {
		return industry;
	}

	public void setIndustry(String industry) {
		this.industry = industry;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getIntroduce() {
		return introduce;
	}

	public void setIntroduce(String introduce) {
		this.introduce = introduce;
	}

	public String getAgreement() {
		return agreement;
	}

	public void setAgreement(String agreement) {
		this.agreement = agreement;
	}

	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public String getCategory02() {
		return category02;
	}

	public void setCategory02(String category02) {
		this.category02 = category02;
	}

	public String getCategory03() {
		return category03;
	}

	public void setCategory03(String category03) {
		this.category03 = category03;
	}

	public String getProductImage() {
		return productImage;
	}

	public void setProductImage(String productImage) {
		this.productImage = productImage;
	}

	public String getProductIcon() {
		return productIcon;
	}

	public void setProductIcon(String productIcon) {
		this.productIcon = productIcon;
	}

	public OpenUser getOpenUser() {
		return openUser;
	}

	public void setOpenUser(OpenUser openUser) {
		this.openUser = openUser;
	}

	public String getProductId() {
		return productId;
	}

	public void setProductId(String productId) {
		this.productId = productId;
	}

